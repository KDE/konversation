msgid "An error occurred while trying to retrieve media metadata from %1."
msgstr "Įvyko klaida bandant gauti kūrinių meta duomenis iš %1."

#: data/scripts/media:257
msgctxt "REGEX_FIXUP is the name of a config key in a config file."
msgid ""
"There is a problem with the regular expression specified in REGEX_FIXUP."
msgstr "Yra problemų su reguliariąja išraiška nurodyta REGEX_FIXUP."

#: data/scripts/media:266
msgid "%1 did not report the metadata needed to output a message."
msgstr "%1 nepateikė meta duomenų reikalingų išvesti pranešimui."

#: data/scripts/media:282
msgid "There is a problem in the output format configuration."
msgstr "Yra problema išvesties formato konfigūracijoje."

#: data/scripts/media:302
msgid "media v%1 for Konversation."
msgstr "Laikmena v%1 skirta Konversation."

#: data/scripts/media:303
msgid "Usage:"
msgstr "Naudojimas:"

#: data/scripts/media:304
msgid "\"/media\" - report what the first player found is playing."
msgstr "„/media“ - pateikia tai ką pirmas rastas grotuvas groja."

#: data/scripts/media:305
msgid ""
"\"/media [ 'audio' | 'video' ]\" - report what is playing in a known audio "
"or video player."
msgstr ""
"„/media [ 'audio' | 'video' ]“ - pateikia tai kas grojama žinomame garso ar "
"vaizdo grotuve."

#: data/scripts/media:306
msgid ""
"\"/media { Player }\" - report what is playing in the specified player if it "
"is found."
msgstr ""
"„/media { Player }“ - pateikia tai kas yra grojama nurodytame grotuve jei "
"jis randamas."

