#: src/identitydialog.ui:471
#, kde-format
msgid "Away command:"
msgstr "Наредба одсутности:"

#. i18n: ectx: property (text), widget (QLabel, unAwayLabel)
#: src/identitydialog.ui:484
#, kde-format
msgid "Return command:"
msgstr "Наредба повратка:"

