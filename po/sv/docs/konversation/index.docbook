					<area id="pt-identity-adv-3" coords="1 1"/>
					<area id="pt-identity-adv-4" coords="1 1"/>
					<area id="pt-identity-adv-5" coords="1 1"/>
					<area id="pt-identity-adv-6" coords="1 1"/>
				</areaspec>
				<imageobject>
					<imagedata fileref="identities_adv_co.png" format="PNG"/>
				</imageobject>
			</imageobjectco>
			<textobject>
