#: data/scripts/cmd:28
msgid "You need to specify a command."
msgstr "Komando bat zehaztu behar duzu."

#: data/scripts/cmd:41
msgid ""
"The command executed successfully, but did not return any output or only "
"whitespace."
msgstr ""
"Komandoa ondo exekutatu da, baina ez du irteerakorik itzuli edo bakarrik "
"zuriuneak itzuli ditu."

#: data/scripts/media:102
msgid "The media script requires Python %1 or higher."
msgstr "Multimedia gidoiak Python %1 edo handiagoa behar du."

#: data/scripts/media:132
msgid "An error occurred while trying to list the running media players."
msgstr ""
"Errore bat gertatu da martxan dauden multimedia jotzaileak zerrendatzen "
"saiatzean."

#: data/scripts/media:176
msgid "No running audio players found."
msgstr "Ez da aurkitu martxan dagoen audio-jotzailerik."

#: data/scripts/media:178
msgid "No running video players found."
msgstr "Ez da aurkitu martxan dagoen bideo-jotzailerik."

#: data/scripts/media:180
msgid "\"%1\" is not running."
msgstr "\"%1\" ez dago martxan."

#: data/scripts/media:182
msgid "No running media players found."
msgstr "Ez da aurkitu martxan dagoen multimedia jotzailerik."

#: data/scripts/media:196
msgid "Nothing is playing in %1."
msgstr "%1(e)an ez da ezer jotzen ari."

#: data/scripts/media:200
msgctxt "1 = Comma-separated list of audio players."
msgid "None of the running audio players (%1) are playing anything."
msgstr "Audio-jotzaileetako batere (%1) ez da ezer jotzen ari."

#: data/scripts/media:205
msgctxt "1 = Comma-separated list of video players."
msgid "None of the running video players (%1) are playing anything."
msgstr "Bideo-jotzaileetako batere (%1) ez da ezer jotzen ari."

#: data/scripts/media:210
msgctxt "1 = Comma-separated list of media players."
msgid "None of the running media players (%1) are playing anything."
msgstr "Multimedia-jotzaileetako batere (%1) ez da ezer jotzen ari."

#: data/scripts/media:218
msgid "An error occurred while trying to retrieve media metadata from %1."
msgstr ""
"Errore bat gertatu da %1(e)tik multimedia metadatuak eskuratzen saiatzean."

#: data/scripts/media:256
msgctxt "REGEX_FIXUP is the name of a config key in a config file."
msgid ""
"There is a problem with the regular expression specified in REGEX_FIXUP."
msgstr ""
"Arazo bat dago «REGEX_FIXUP»en zehaztutako adierazpen erregularrarekin."

#: data/scripts/media:265
msgid "%1 did not report the metadata needed to output a message."
msgstr "%1(e)k ez ditu jakinarazi mezu bat igortzeko beharrezko metadatuak."

#: data/scripts/media:281
msgid "There is a problem in the output format configuration."
msgstr "Arazo bat dago irteerako formatuaren konfigurazioan."

#: data/scripts/media:301
msgid "media v%1 for Konversation."
msgstr "Konversation-entzako media v%1."

#: data/scripts/media:302
msgid "Usage:"
msgstr "Erabilera:"

#: data/scripts/media:303
msgid "\"/media\" - report what the first player found is playing."
msgstr "\"/media\" - eman aurkitutako aurreneko jotzailea zer jotzen ari den."

#: data/scripts/media:304
msgid ""
"\"/media [ 'audio' | 'video' ]\" - report what is playing in a known audio "
"or video player."
msgstr ""
"\"/media [ 'audio' | 'video' ]\" - eman audio edo bideo jotzaile ezagun "
"batean jotzen ari dena."

#: data/scripts/media:305
msgid ""
"\"/media { Player }\" - report what is playing in the specified player if it "
"is found."
msgstr ""
"\"/media { Jotzailea }\" - eman, aurkituz gero, zehaztutako jotzailean "
"jotzen ari dena."

