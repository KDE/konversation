#: data/scripts/media:303
msgid "Usage:"
msgstr "Upotreba:"

#: data/scripts/media:304
msgid "\"/media\" - report what the first player found is playing."
msgstr "\"/media\" - prijavi šta prvi nađeni izvođač izvodi."

#: data/scripts/media:305
msgid ""
"\"/media [ 'audio' | 'video' ]\" - report what is playing in a known audio "
"or video player."
msgstr ""
"\"/media [ 'audio' | 'video' ]\" - prijavi šta se izvodi u poznatom audio "
"ili video izvođaču."

#: data/scripts/media:306
msgid ""
"\"/media { Player }\" - report what is playing in the specified player if it "
"is found."
msgstr ""
"\"/media { Player }\" - izvijesti šta se izvodi u navedenom izvođaču ako je "
"pronađeno."

